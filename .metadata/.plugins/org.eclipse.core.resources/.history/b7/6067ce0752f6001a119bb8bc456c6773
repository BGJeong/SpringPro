package java0914;

import java.util.Scanner;

class MemberInput{
	String name, age, email, addr;
	public MemberInput() {
		System.out.println("이름, 나이, 이메일, 주소를 입력");
		Scanner scan = new Scanner(System.in);
		name = scan.nextLine();
		age = scan.nextLine();
		email = scan.nextLine();
		addr = scan.nextLine();
	}
	void Input() {
		System.out.println(name+" "+ age+" "+ email+" "+ addr);
	}
}
class MemberInfo{
	String name, age, email, addr;
	public MemberInfo(String name, String age, String email, String addr) {
		this.name = name;
		this.age = name;
		this.email = name;
		this.addr = name;
	}
	void Output() {
		
	}
}

public class Exam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MemberInput meminput = new MemberInput();
		MemberInput.Input();
	}

}
