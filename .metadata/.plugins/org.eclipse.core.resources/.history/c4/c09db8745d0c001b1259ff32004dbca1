show databases;

create table customer(id int(4) auto_increment primary key, 
		       name varchar(20),
		       email varchar(20),
		       tel varchar(20),
		       address varchar(50),
		       reg_date timestamp);

		       
create table board(
	no int auto_primary primary key,
	writer varchar(20) not null,
    passwd varchar(20) not null,
	subject varchar(100) not null,
	content varchar(1000) not null,
	reg_date timestamp );