package java0929;
class howlikethat{
	public howlikethat(int a, int b) {
		System.out.println(a+b);
	}
	public int ogg(int a, int b) {
		return a*b;
	}
}
public class Exam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		howlikethat hlt = new howlikethat(10, 20);
		hlt.ogg(20, 10);
	}

}
