package java0915;
class Car2 {
	String model;
	int speed;
	
	Car2(String model){
		this.model = model;
	}
	
	void setSpeed(int speed) {
		this.speed = speed; 
	}
	void run() {
		for(int i = 0; i <= 50; i++) {
			this.setSpeed(i);
			System.out.println(this.model + "가 달립니다. (시속 :" + this.speed+"km/h)");
		}
	}
}
public class CarEx2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
