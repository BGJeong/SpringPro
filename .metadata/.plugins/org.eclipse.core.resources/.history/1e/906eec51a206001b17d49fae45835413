package java1005;

public class Exam {
	public static void main(String[] args) {
		try {
			int[] lot = new int[6];
			for (int i = 0; i < lot.length; i++) {
				lot[i] = (int) (Math.random() * 45 + 1);
				for (int j = 0; j < i; j++) {
					if (lot[j] == lot[i]) {
						i--;
						break;
					}
				}
			}
			for (int i = 0; i < lot.length; i++) {
				for (int j = 0; j < lot.length-i ; j++) {
					if (lot[j] > lot[j + 1]) {
						int temp = lot[j + 1];
						lot[j + 1] = lot[j];
						lot[j] = temp;
					}
				}
			}
			for (int i : lot)
				System.out.println(i);

		} catch (Exception e) {
			System.out.println(e.getMessage());
		}
	}
}
