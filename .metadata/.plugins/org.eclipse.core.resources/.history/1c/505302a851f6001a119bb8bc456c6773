package java0914;

import java.util.Scanner;

class MemberInput{
	String name, age, email, addr;
	public MemberInput() {
		Scanner scan = new Scanner(System.in);
		name = scan.nextLine();
		age = scan.nextLine();
		email = scan.nextLine();
		addr = scan.nextLine();
	}
	void Input() {
		
	}
}
