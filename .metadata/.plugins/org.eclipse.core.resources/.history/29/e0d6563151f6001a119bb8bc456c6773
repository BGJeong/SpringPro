package java0914;

import java.util.Scanner;

class MemberInfo{
	String name, age, email, addr;
	public MemberInfo() {
		Scanner scan = new Scanner(System.in);
		name = scan.nextLine();
		age = scan.nextLine();
		email = scan.nextLine();
		addr = scan.nextLine();
	}
}

