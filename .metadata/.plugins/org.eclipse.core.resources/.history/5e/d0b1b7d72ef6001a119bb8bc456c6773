package java0914;

class CarDemo{
	String name;
	int number;
	
	public CarDemo() {
		name = "이름없음";
		number = 0;
	}
	public CarDemo(String a) {
		name = a;
		number = 0;
	}
	public CarDemo(String a, int b) {
		name = a;
		number = b;
	}
	public CarDemo(int a) {
		name = "이름없음";
		number = a;
	}
	public void Car2() {
		System.out.println("Car2");
	}
}

public class Car {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		CarDemo Car = new CarDemo(1);
		CarDemo Car2 = new CarDemo("소방차");
		CarDemo Car3 = new CarDemo();
		CarDemo Car4 = new CarDemo("경찰차",1);
		System.out.println(Car.name +" " +Car.number);
		System.out.println(Car2.name +" " +Car2.number);
		Car.Car2();
		System.out.println(Car3.name +" " +Car3.number);
		System.out.println(Car4.name +" " +Car4.number);
	}

}
